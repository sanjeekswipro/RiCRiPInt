% Font name map for Latin text in T1 fonts
% This is the default map for fonts used in print marginalia
% in Latin installations

/FontNameMap <<

	/Fonts <<
		/Plain	/Helvetica
		/Emphasis	/Helvetica-Bold
		/Narrow	/Helvetica-Narrow
		/NarrowEmphasis	/Helvetica-Narrow-Bold
		/Monospace	/Courier
		/MonospaceEmphasis	/Courier-Bold
	>>
	
	/FontClass	(T1Latin)
	
>>

%%EOF
