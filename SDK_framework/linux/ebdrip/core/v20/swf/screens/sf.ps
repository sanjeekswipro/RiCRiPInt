% Chain screen suitable for 6-color printing

/HexChain <<
  /HalftoneType 5
  /HalftoneName /HexChain
  /OverrideAngle false
  /InheritAngle false

  (Hex Cyan) cvn <<
    /HalftoneType 1
    /Frequency 127
    /Angle -63.43494882292
    /ScreenChanges { exch 1.11803398875 mul exch } bind
    /SpotFunction {dup mul exch dup mul 0.05 mul add 1 exch sub}
  >>

  (Hex Magenta) cvn <<
    /HalftoneType 1
    /Frequency 127
    /Angle 63.43494882292
    /ScreenChanges { exch 1.11803398875 mul exch } bind
    /SpotFunction {dup mul exch dup mul 0.05 mul add 1 exch sub}
  >>

  (Hex Yellow) cvn <<
    /HalftoneType 1
    /Frequency 127
    /Angle 0
    /SpotFunction {dup mul exch dup mul 0.05 mul add 1 exch sub}
  >>

  (Hex Black) cvn <<
    /HalftoneType 1
    /Frequency 127
    /Angle 26.56505117708
    /ScreenChanges { exch 1.11803398875 mul exch } bind
    /SpotFunction {dup mul exch dup mul 0.05 mul add 1 exch sub}
  >>

  /Default <<
    /HalftoneType 1
    /Frequency 127
    /Angle 26.56505117708
    /ScreenChanges { exch 1.11803398875 mul exch } bind
    /SpotFunction {dup mul exch dup mul 0.05 mul add 1 exch sub}
  >>

  (Hex Orange) cvn <<
