%!PS-Adobe-3.0 Resource-Font
%%Version: 002.001 (1)
%%VMlocation: global

(%os%shivafonts/Turku-BoldItalic) run

/Times-BoldItalic
/Turku-BoldItalic findfont dup length dict begin
% start of loop for original Font dictionary keys
{exch dup /FontInfo eq{ exch dup length dict begin
% start of loop for the /FontInfo dictionary keys
 {exch dup /FullName eq {(Times-BoldItalic) def pop}
%not /FullName
 {dup /FamilyName eq{(Times) def pop}{exch def}ifelse}ifelse}
% end of loop for the /FontInfo dictionary keys
 forall currentdict readonly end def}
%not /FontInfo
{exch def}ifelse}
% end of loop forall original Font dictionary keys
forall

/Turku-BoldItalic undefinefont
dup /FontName exch def
currentdict /SubstFontName undef
currentdict end
dup /FID undef
definefont pop
