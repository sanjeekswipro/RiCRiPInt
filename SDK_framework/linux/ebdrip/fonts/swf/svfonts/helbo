%!PS-Adobe-3.0 Resource-Font
%%Version: 002.001 (1)
%%VMlocation: global

(%os%shivafonts/Helsinki-Bold) run

/Helvetica-BoldOblique
/Helsinki-Bold findfont dup length dict begin
% start of loop for original Font dictionary keys
{exch dup /FontInfo eq{ exch dup length dict begin
% start of loop for the /FontInfo dictionary keys
 {exch dup /FullName eq {(Helvetica-BoldOblique) def pop}
%not /FullName
 {dup /FamilyName eq{(Helvetica) def pop}{exch def}ifelse}ifelse}
% end of loop for the /FontInfo dictionary keys
 forall currentdict readonly end def}
%not /FontInfo
{exch def}ifelse}
% end of loop forall original Font dictionary keys
forall
/Helsinki-Bold undefinefont
/UniqueID 5087422 def
dup /FontName exch def
currentdict /SubstFontName undef
/FontMatrix [0.001 0 0.212557E-3 0.001 0 0] readonly def
currentdict end
dup /FID undef
definefont pop

