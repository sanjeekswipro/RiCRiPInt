%!PS-Adobe-3.0 Resource-Font
%%Version: 002.001 (1)
%%VMlocation: global

(%os%shivafonts/Aavasaksa-Demi) run

/AvantGarde-DemiOblique
/Aavasaksa-Demi findfont dup length dict begin
% start of loop for original Font dictionary keys
{exch dup /FontInfo eq{ exch dup length dict begin
% start of loop for the /FontInfo dictionary keys
 {exch dup /FullName eq {(AvantGarde-DemiOblique) def pop}
%not /FullName
 {dup /FamilyName eq{(AvantGarde) def pop}{exch def}ifelse}ifelse}
% end of loop for the /FontInfo dictionary keys
 forall currentdict readonly end def}
%not /FontInfo
{exch def}ifelse}
% end of loop forall original Font dictionary keys
forall
/Aavasaksa-Demi undefinefont
/UniqueID 5087414 def
dup /FontName exch def
currentdict /SubstFontName undef
/FontMatrix [0.001 0 0.185339E-3 0.001 0 0] readonly def
currentdict end
dup /FID undef
definefont pop

